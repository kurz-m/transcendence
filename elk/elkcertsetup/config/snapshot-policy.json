{
    "name": "daily-snapshots",
    "schedule": "0 0 0 * * ?",
    "repository": "transcendence_logbackup",
    "config": {
    "indices": ["transcendence-logs"],
    "ignore_unavailable": false,
    "include_global_state": false
    },
    "retention": {
    "expire_after": "30d",
    "min_count": 5,
    "max_count": 50
    }
}
